{
    "cSpell.words": [
        "Cupertino",
        "Jetpack",
        "Multiplatform",
        "Skia"
    ],
    "cSpell.language": "en,cs",
    "cSpell.enableFiletypes": [
        "julia"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-outdir=%OUTDIR%",
                "%DOC%"
            ],
        },
    ]
}
